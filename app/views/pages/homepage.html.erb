<div class="row">
    <div class="jumbotron">
        <div class="col-xs-5 hidden-xs">
            <h2>A support network for entrepreneurial women around the world.</h2>
        </div>
    </div>
</div>
<section class="row">
    <div class="col-xs-12">
        <h2>Mentor advice videos</h2>
        <hr>
    </div>
    <div class="profile col-lg-4 col-md-6">
      <%= video_tag 'www.youtube.com/embed/99TdS8qXnrg' %>
      <h2>Kerry Leary: Social Media</h2>
    </div>

    <div class="profile col-lg-4 col-md-6">
        <%= link_to kinsey_path do %>
           <%= image_tag 'kinsey_tile.png',:title => 'Kinsey' %>
           <h2>Kinsey</h2>
        <% end %>
    </div>

    <div class="profile col-lg-4 col-md-6">
        <%= link_to brooke_path do %>
           <%= image_tag 'brooke_team.png',:title => 'Brooke' %>
           <h2>Brooke</h2>
        <% end %>
    </div>
</section>

<section id="how_it_works" class="row">
    <div class="col-xs-12">
        <h2>The Workshop</h2>
        <hr>
    </div>


    <div class="col-md-7">
        <%= image_tag 'biztech.jpg',:title => 'Biztech workshop', :style => "width: 100%;"  %>
    </div>
    <div class="col-md-5">
            <div class="col-xs-2">
                <%= image_tag 'phone_small_icon.png', :title => 'icon', :class => "img-responsive step"  %>
            </div>
            <div class="col-xs-10">
                <h3>The BizTech Workshop</h3>
                <p>Training in Nairobi, Kenya from mentors on entrepreneurism. Each participant recieves a phone package.</p>
            </div>
            <div class="col-xs-2">
                <%= image_tag 'year_long_icon.png', :title => 'icon', :class => "img-responsive step" %>
            </div>
            <div class="col-xs-10">
                <h3>Year-long support</h3>
                <p>Mentorship from other entrepreneurs, a phone package and marketing through Kubmo</p>
            </div>
            <div class="col-xs-2">
                <%= image_tag 'marketplace_icon.png',:title => 'icon', :class => "img-responsive step"  %>
            </div>
            <div class="col-xs-10">
                <h3>Global network</h3>
                <p>Kubmo connects women around the world to create sustainable relationships around social entrepreneurism. </p>
            </div>
    </div>
</section>

